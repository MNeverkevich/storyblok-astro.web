{
  "extends": "astro/tsconfigs/strict",
  "exclude": [
    "node_modules/"
  ],
  "compilerOptions": {
    "jsx": "react-jsx",
    "esModuleInterop": true,
    "allowJs": true,
    "baseUrl": ".",
    "paths": {
      "@storyblok/*": [
        "src/storyblok/*"
      ],
      "@pages/*": [
        "src/pages/*"
      ],
    }
  }
}
